
@{
    ViewData["Title"] = "Profilim";
}
@model HospitalAppointment.DTO.DTOs.PatientRegistrar.ActivePatientRegistrarDto

<div class="col-md-10 container-fluid">
    <div class="card">
        <div class="card-header">
            <h5>Departman Ekleme Sayfası</h5>
        </div>
        <div class="card-block">
            <form class="form-material" asp-action="" method="post">
                <input type="hidden" asp-for="RolId" value="@Model.RolId" />
                <input type="hidden" asp-for="UserId" value="@Model.UserId" />
                <div class="form-group form-default">
                    <input type="text" class="form-control" required disabled asp-for="Name" value="@Model.Name">
                    <span class="form-bar"></span>
                </div>
                <div class="form-group form-default">
                    <input type="text" class="form-control" required disabled asp-for="SurName" value="@Model.SurName">
                    <span class="form-bar"></span>
                </div>
                <div class="form-group form-default">
                    <input type="text" class="form-control" required disabled asp-for="Tcno" value="@Model.Tcno">
                    <span class="form-bar"></span>
                </div>
                <div class="form-group form-default">
                    <input type="text" class="form-control" required disabled asp-for="Telephone" value="@Model.Telephone">
                    <span class="form-bar"></span>
                </div>
          
                <br />
                <button type="submit" class="btn waves-effect waves-light btn-primary"><i class="icofont icofont-check-circled"></i>Kaydet</button>
            </form>
        </div>
    </div>
</div>